<header>

    <span>
        <a href="/"> THE OMNIOLOGY COLLECTION </a>
    </span>

    <menu>
        <a id="week1anc" class="weekBtn" href="/week/1">Week 1</a>
        <a id="week2anc" class="weekBtn" href="/week/2">Week 2</a>
        <a id="week3anc" class="weekBtn" href="/week/3">Week 3</a>
        <a id="week4anc" class="weekBtn" href="/week/4">Week 4</a>
        <div id="cartDiv">
            <a id="cartIcon" href="/cart"><img src="/images/shopping-cart-24.png"></a>
            <% if (typeof cart !== 'undefined' && cart.length > 0) { %>
                <span id="cartNum"><%= cart.length %></span>
            <% } %>
        </div>
        <div class="dropdown">
            <span class="dropbtn">Account</span>
            <div class="dropdown-content">
                <% if (!currentUser) { %>
                <a href="/register">Register</a>
                <a href="/login">Login</a>
                <% } else { %>
                <a href="/logout">Logout</a>
                <% } %>
            </div>
        </div>
    </menu>
</header>